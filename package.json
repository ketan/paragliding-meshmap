{
  "name": "paragliding-meshmap",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf build frontend/dist",
    "build": "concurrently yarn:build:fe yarn:build:be && cp -rf frontend/dist build/public",
    "build:be": "rm -rf build && npx rollup -c rollup.config.js",
    "build:fe": "cd frontend && rm -rf ./dist && tsc -b && DOTENV_FLOW_PATH=$(pwd)/.. node -r dotenv-flow/config ../node_modules/.bin/vite build --mode=production",
    "start": "concurrently -P \"yarn:start:be -- {@}\" yarn:start:fe --",
    "start:be": "export NODE_ENV=development; nodemon --exec node -r dotenv-flow/config --loader ts-node/esm --enable-source-maps src/index.ts --",
    "start:fe": "export NODE_ENV=development; cd frontend && DOTENV_FLOW_PATH=$(pwd)/.. node -r dotenv-flow/config ../node_modules/.bin/vite dev",
    "mqtt": "export NODE_ENV=development; nodemon --exec node -r dotenv-flow/config --loader ts-node/esm --enable-source-maps src/mqtt.ts --",
    "prod": "node -r index.js",
    "lint": "eslint .",
    "format": "prettier --write .",
    "fmt": "yarn run format",
    "typeorm": "NODE_ENV=development typeorm-ts-node-esm -d src/config/data-source.ts",
    "migrate": "NODE_ENV=development yarn run typeorm migration:run",
    "schema-to-ts": "bin/schema-to-ts",
    "docker:build": "docker build . --progress plain --tag paragliding-meshmap --build-arg GIT_SHA=$(git rev-parse --short HEAD) --tag ghcr.io/ketan/paragliding-meshmap:latest",
    "test": "c8 mocha",
    "test:watch": "nodemon --config nodemon.test.json --exec mocha",
    "precommit": "concurrently yarn:fmt yarn:lint yarn:build yarn:test"
  },
  "imports": {
    "#entity/*": "./src/entity/*.ts",
    "#migrations/*": "./src/migration/*.ts",
    "#mqtt/*": "./src/mqtt/*.ts",
    "#config/*": "./src/config/*.ts",
    "#helpers/*": "./src/helpers/*.ts",
    "#web/*": "./src/web/*.ts"
  },
